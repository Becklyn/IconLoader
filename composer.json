{
    "name": "becklyn/icon-loader",
    "description": "Icon Loader: loads (svg) icons from different directories, provides a twig function and caches them.",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "keywords": [
        "becklyn",
        "symfony",
        "icons",
        "loader",
        "cache"
    ],
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "homepage": "https://github.com/Becklyn/IconLoader",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/cache": "^5.4.3 || ^6.0.3",
        "symfony/cache-contracts": "^2.0.0 || ^3.0.0",
        "symfony/config": "^5.4.3 || ^6.0.3",
        "symfony/console": "^5.4.3 || ^6.0.3",
        "symfony/dependency-injection": "^5.4.3 || ^6.0.3",
        "symfony/finder": "^5.4.3 || ^6.0.3",
        "symfony/http-kernel": "^5.4.4 || ^6.0.4",
        "twig/twig": "^3.3.8"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4.3 || ^6.0.3",
        "symfony/yaml": "^5.4.3 || ^6.0.3"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\IconLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\IconLoader\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
