{
    "name": "becklyn/icon-loader",
    "type": "symfony-bundle",
    "description": "Icon Loader: loads (svg) icons from different directories, provides a twig function and caches them.",
    "keywords": [
        "becklyn",
        "symfony",
        "icons",
        "loader",
        "cache"
    ],
    "homepage": "https://github.com/Becklyn/IconLoader",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/cache": "^4.2 || ^5.0",
        "symfony/cache-contracts": "^1.1 || ^2.0",
        "symfony/config": "^4.2 || ^5.0",
        "symfony/console": "^4.3 || ^5.0",
        "symfony/dependency-injection": "^4.2 || ^5.0",
        "symfony/finder": "^4.2 || ^5.0",
        "symfony/http-kernel": "^4.2 || ^5.0",
        "twig/twig": "^2.7 || ^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\IconLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\IconLoader\\": "tests/"
        }
    }
}
